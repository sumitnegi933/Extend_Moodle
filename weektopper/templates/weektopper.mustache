<div id="weektoppercarousel" class="carousel slide" data-ride="carousel">
 <div class="carousel-inner">
{{#toppers}}

    {{#quiz}}
    <div class="carousel-item {{slidestatus}}">
        <div class="tw-title">
            ( <a href='#'>{{name}}</a> )
        </div>
        <div class="row">
        {{#users}}
            <div class="col-6 tw-item">
                {{#userpicture}}
                    {{{userpicture}}}
                {{/userpicture}}
                <p>{{fullname}}</p>
            </div>
        {{/users}}
        </div>
    </div>
    {{/quiz}}

{{/toppers}}
</div>
 <a class="carousel-control-prev" href="#weektoppercarousel" role="button" data-slide="prev">
    <i class="fa fa-chevron-left" aria-hidden="true"></i>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
    <i class="fa fa-chevron-right" aria-hidden="true"></i>
    <span class="sr-only">Next</span>
  </a>
</div>


